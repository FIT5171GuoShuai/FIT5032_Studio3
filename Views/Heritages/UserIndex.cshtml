@model IEnumerable<FIT5032_MyJs.Models.Heritage>

@{
    ViewBag.Title = "Index";
}

<h2>Heritage</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.hname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pic)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.hname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pic)
        </td>
       
    </tr>
}

</table>
